<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Birth Date</th>
      <th>Zip</th>
      <th>User #</th>    
      
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.name, show_user_path(user) %></td>
        <td><%= user.birthdate %></td>
        <td><%= user.zip %></td>
        <td><%= user.id %></td>  
    
          
      </tr>
    <% end %>
  </tbody>
</table>

<br>


